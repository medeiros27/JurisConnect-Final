# Configuração para Vite (frontend/.env)
# IMPORTANTE: Variáveis para Vite devem começar com VITE_

# URL da API Backend
VITE_API_URL=http://localhost:3001/api

# Configuração de Desenvolvimento
VITE_ENV=development
VITE_DEBUG=true

# Configuração de Timeout
VITE_API_TIMEOUT=10000

# Configuração de Health Check
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_HEALTH_CHECK_TIMEOUT=5000

# URLs de Fallback para teste
VITE_API_FALLBACK_URLS=http://localhost:3000/api,http://localhost:3001/api,http://localhost:3002/api

# Compatibilidade com React (opcional)
REACT_APP_API_URL=http://localhost:3002/api

